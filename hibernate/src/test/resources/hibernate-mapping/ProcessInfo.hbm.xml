<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.greenbee.tutorials.hibernate.model.ProcessInfo" table="vt_processinfo">
        <composite-id name="componentKey" class="com.greenbee.tutorials.hibernate.model.ComponentKey">
            <key-property name="instanceId" type="string" column="instanceId" length="120"/>
            <key-property name="componentName" type="string" column="componentName" length="120"/>
        </composite-id>
        <version name="versionNum" column="version" type="integer"/>
        <property name="bpState" type="string" column="bpstate"/>
        <property name="bpControlState" type="integer" column="bpcontrolstate"/>
        <property name="numTransitions" type="integer" column="numtransitions"/>
        <property name="timeEntered" type="timestamp" column="timeentered"/>
        <property name="timeCreated" type="timestamp" column="timecreated"/>
        <property name="bpVariableSet" type="blob" column="bpvariableset"/>
    </class>
</hibernate-mapping>
